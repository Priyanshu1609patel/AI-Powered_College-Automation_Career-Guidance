  "chatbot_intents": [
    {
      "intent": "greeting",
      "patterns": ["hi", "hello", "hey", "hii", "helo", "start", "help", "namaste"],
      "response": "Hello! I am your CSE Study Bot for Indus University. Ask me for study material or practicals for any subject!"
    },
    {
      "intent": "material_request",
      "patterns": ["material", "study material", "notes", "pdf", "resources", "drive", "link"],
      "logic": "Match subject name or code from user query -> find semester -> return drive_link for that semester"
    },
    {
      "intent": "practical_request",
      "patterns": ["practical", "lab", "experiment", "prac", "lab file", "practical file"],
      "logic": "Match subject name or code from user query -> find semester -> return drive_link for that semester. If subject has_practical is false, inform user."
    },
    {
      "intent": "semester_subjects",
      "patterns": ["sem 1 subjects", "sem 2 subjects", "list subjects", "all subjects", "subjects in sem"],
      "logic": "Extract semester number from query -> return all subjects for that semester from semesters array"
    },
    {
      "intent": "semester_material",
      "patterns": ["sem 1 material", "sem 2 notes", "semester 3 drive", "all material sem"],
      "logic": "Extract semester number -> return drive_link for that semester"
    },
    {
      "intent": "minor_track",
      "patterns": ["minor", "minor track", "minor subject"],
      "response_link": "https://drive.google.com/drive/folders/13H2TyB8GNzecGK96zR2cZWBoUH8fG2hJ?usp=drive_link"
    }
  ],

  "response_logic": {
    "description": "How the chatbot should respond to user queries",
    "steps": [
      "1. Normalize user input to lowercase",
      "2. Check if query matches any subject keyword from the subjects list",
      "3. If match found, get the semester number of that subject",
      "4. Return the drive_link for that semester",
      "5. If query contains 'practical' or 'lab', mention practical files; else mention study material",
      "6. If no subject match, check if query mentions a semester number directly",
      "7. If semester number found, return that semester's drive_link",
      "8. If query mentions 'minor' or 'minor track', return minor_track drive link",
      "9. If nothing matches, ask user to rephrase with subject name or code"
    ],
    "note": "Semester 8 is excluded as it only contains the final project (CE0816)"
  }
}
